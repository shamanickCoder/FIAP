package br.com.fiap.beans;

public class Cliente {
	private String nome = new String();
	private byte qtdeEstrelas = 0;
	private long codigo = 0;

	public Cliente() {
	}

	public Cliente(String nome, byte qtdeEstrelas, int codigo) {
		this.nome = nome;
		this.qtdeEstrelas = qtdeEstrelas;
		this.codigo = codigo;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	public byte getQtdeEstrelas() {
		return qtdeEstrelas;
	}

	public void setQtdeEstrelas(byte qtdeEstrelas) {
		this.qtdeEstrelas = qtdeEstrelas;
	}

	public long getCodigo() {
		return codigo;
	}

	public void setCodigo(long codigo) {
		this.codigo = codigo;
	}

}

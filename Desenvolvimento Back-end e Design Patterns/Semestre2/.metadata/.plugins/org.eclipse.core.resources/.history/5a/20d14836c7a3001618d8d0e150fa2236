package br.com.fiap.teste;

public class Excecao_01 {
	public static void main(String[] args) {
		int nota = 10;
		int div = 0;
		int resultado = 0;
		try{
			resultado = nota / div;
		} catch(Exception e){
			System.out.println("Calculo não realizado - " + e);
		} finally{
			System.out.println("Nota: " + nota);
			System.out.close();
		}
		
	}
}
